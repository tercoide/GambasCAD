' Gambas class file

' Gambasclass file
Public Xreal As Float
Public Yreal As Float
Public EntitiesBelow As Entity[]

Inherits Task

Public Sub Main()

    Dim c As Integer

    Debug "Searching at " & Str(Xreal) & ";" & Str(Yreal)
    Debug "Total entities to search for " & Str(gcd.Drawing.EntitiesVisibles.Count)
    c = gcd.Drawing.EntitiesVisibles.Count
    Do
        EntitiesBelow = clsMouseTracking.CheckAboveEntities(Xreal, Yreal)
        If EntitiesBelow.Count > 0 Then
            Debug "Found something at " & Str(Xreal) & ";" & Str(Yreal)

        Endif
        If gcd.Drawing.EntitiesVisibles.Count <> c Then
            c = gcd.Drawing.EntitiesVisibles.Count
            Debug "Total entities to search for now is " & Str(gcd.Drawing.EntitiesVisibles.Count)
        Endif
    Loop

    Return

Catch
    Debug "Searching failed: " & Error.Text

End
