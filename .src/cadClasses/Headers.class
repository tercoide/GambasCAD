Public Function SetValues(sVarName As String, vValue As Variant[]) As Boolean

    Dim slx As New Single[]
    Dim inx As New Integer[]
    Dim Tipo As Integer
    Dim v As Variant
    Dim sVarName2 As String

    If IsNumber(Left(sVarName, 1)) Then sVarName2 = "__" & sVarName Else sVarName2 = sVarName

    If vValue.Count > 1 Then
        ' determino el tipo de array
        tipo = Object.GetProperty(Me, "_" & sVarName)
        If (tipo > 9) And (tipo < 70) Then
            For Each v In vValue
                slx.Add(v)
            Next
            Object.SetProperty(Me, sVarName2, slx)
        Else
            For Each v In vValue
                inx.Add(v)
            Next
            Object.SetProperty(Me, sVarName2, inx)
        End If

    Else

        Object.SetProperty(Me, sVarName2, vValue[0])

    Endif
    Return True
Catch
    Return False

End

'' Devuelve una string apta DXF
Public Function ExportDXF() As String[]

    Dim stx As New String[]
    Dim i, tipo, iVal As Integer
    Dim obj As Object = Me
    Dim MyClass As Class = Object.Class(obj)
    Dim Var, var2 As String
    Dim Valor As Variant
    Dim slx As New Single[]
    Dim inx As New Integer[]
    Dim vlx As New Variant[]
    Dim sl As Single

    For Each Var In myClass.Symbols
        If InStr(var, "_") > 0 Then Continue
        stx.Add("9")
        stx.Add("$" & var)

        '' Verifying that it is a property or a variable.
        If (MyClass[var].kind = Class.Variable) Or (MyClass[var].kind = Class.Property) Then
            tipo = Object.GetProperty(obj, "_" & var)
            If MyClass[var].Type = "Single[]" Or MyClass[var].Type = "Integer[]" Then 'is an array

                If tipo = 10 Then ' es un array
                    slx = Object.GetProperty(Me, var)
                    stx.Add("10")
                    If slx.Count >= 1 Then stx.Add(slx[0]) Else stx.Add(0)
                    stx.Add("20")
                    If slx.Count >= 2 Then stx.Add(slx[1]) Else stx.Add(0)
                    If slx.Count = 3 Then
                        stx.Add("30")
                        If slx.Count >= 3 Then stx.Add(slx[2]) Else stx.Add(0)
                    End If
                Else If tipo = 70 Then
                    inx = Object.GetProperty(Me, var)
                    For Each iVal In inx
                        stx.Add("70")
                        stx.Add(i)
                    Next

                Else If tipo = 40 Then
                    slx = Object.GetProperty(Me, var)
                    For Each sl In slx
                        stx.Add("40")
                        stx.Add(sl)
                    Next

                Endif
            Else
                stx.Add(CStr(tipo))
                stx.Add(Object.GetProperty(Me, var))
            End If
        End If

    Next

    Return stx

End
