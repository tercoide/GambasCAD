Public Sub VBOFlush()

    VBO_vertex.Clear
    VBO_colors.Clear
    VBO_normals.Clear
    VBO_pixels.Clear

    VBO_vertex = New Single[]
    VBO_colors.Clear
    VBO_normals.Clear
    VBO_pixels.Clear

