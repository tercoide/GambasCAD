Public Function GetLTypeByName(sName As String) As LineType

    Dim LT As LineType

    For Each LT In gcd.Drawing.LineTypes

        If LCase(LT.Name) = LCase(sname) Then Return LT

    Next

    Return Null

End

Public Function GetLTypeByIndex(sName As String) As LineType

    Dim lt As LineType

    For Each lt In gcd.Drawing.LineTypes

        If LCase(lt.Name) = LCase(sname) Then Return lt

    Next

    Return Null

End

