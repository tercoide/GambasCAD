Public Function SelectionSquare(X0 As Float, Y0 As Float, x1 As Float, y1 As Float, Optional crossing As Boolean = False) As Collection

    ' Veo que accion estoy tomando
    Dim e As Entity, i, i2, TotalSeleccionados As Integer
    Dim c As New Collection
    Dim lay As Layer

    Dim tot As Integer
    Dim s As Sheet
    Dim ho As String
    Dim t As Timer
    Dim insideX, insideY, OutSideX, OutSidey As Boolean

    For Each e In gcd.Drawing.Sheet.EntitiesVisibles

        insideX = False
        insideY = False
        OutSideX = False
        OutSidey = False

        If (e.Limits[0] >= x0) And (e.Limits[2] <= x1) Then insideX = True
        If (e.Limits[1] >= y0) And (e.Limits[3] <= y1) Then insidey = True
        If (e.Limits[0] > x1) Or (e.Limits[2] < x0) Then OutSideX = True
        If (e.Limits[1] > y1) Or (e.Limits[3] < y0) Then OutSidey = True

        If insideX And insideY Then

            c.Add(e, e.id)
            e.Psel.Fill(True)

        Else If OutSideX Or OutSidey Then
            Continue

        Else
            ' cada entidad debe decidir si esta seleccionada o no
            If crossing Then
                If SelPArtial(e, X0, Y0, X1, Y1) Then c.Add(e, e.id)

            Else

                If SelFull(e, X0, Y0, X1, Y1) Then c.Add(e, e.id)
            End If
        End If
    Next

    Return c

End

Public Function SelectionPoly(Poly As Float[], Optional crossing As Boolean = False) As Collection

    ' Veo que accion estoy tomando
    Dim e As Entity
    Dim c As New Collection

    For Each e In gcd.Drawing.Sheet.EntitiesVisibles
        ' cada entidad debe decidir si esta seleccionada o no
        If crossing Then
            If gcd.CCC[e.Gender].SelPartialPoly(e, Poly) Then c.Add(e, e.id)

        Else
            If gcd.CCC[e.Gender].SelFullPoly(e, Poly) Then c.Add(e, e.id)

        End If
    Next

    Return c

End

'' Put all visible entity refs in arrVisibleEntities[]
Public Function CollectVisibleEntities(Optional sSheet As Sheet)

    Dim x0, y0, x1, y1 As Float
    Dim lay As Layer
    Dim e As Entity
    Dim i, tot As Integer
    Dim s As Sheet
    Dim ho As String
    Dim t As Timer
    Dim insideX, insideY As Boolean

    gcd.ResetChronograph

    x0 = gcd.Xreal(0)
    y1 = gcd.Yreal(0)                     ' el eje Y esta invertido en OpenGL
    x1 = gcd.Xreal(gcd.ScreenWidth())
    y0 = gcd.Yreal(gcd.ScreenHeight())

    If Not sSheet Then sSheet = gcd.Drawing.Sheet

    'If gcd.Drawing.oEntities > gcd.Drawing.arrVisibleEntities.Max Then gcd.Drawing.arrVisibleEntities.Resize(gcd.Drawing.oEntities.Count)
    gcd.Drawing.Sheet.EntitiesVisibles.Clear

    For Each e In sSheet.Entities
        If e.pLayer.Visible Then
            insideX = False
            insideY = False
            If (e.Limits[0] <= x1) And (e.Limits[2] >= x0) Then insideX = True
            If (e.Limits[1] <= y1) And (e.Limits[3] >= y0) Then insidey = True

            If insideX And insideY Then

                gcd.Drawing.Sheet.EntitiesVisibles.Add(e, e.id)

            End If

        End If
    Next

    gcd.debugInfo("Recolectadas las entidades visibles " & Str(gcd.Drawing.Sheet.EntitiesVisibles.count) & " de " & Str(gcd.Drawing.Sheet.Entities.count),,, True, True)

End

